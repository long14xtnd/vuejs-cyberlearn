, user }; //return ở đây để cho template có thể lấy được dữ liệu và sử dụng interpolation\r\n  },\r\n  // data() {\r\n  //     return {\r\n  //         username : \"Đoàn Hải Long\"\r\n  //     }\r\n  // },\r\n  // created(){\r\n  //     setTimeout(() =>{\r\n  //         this.username=\"Đoàn Minh Hải\"\r\n  //     },2000)\r\n  // }\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"]}]}