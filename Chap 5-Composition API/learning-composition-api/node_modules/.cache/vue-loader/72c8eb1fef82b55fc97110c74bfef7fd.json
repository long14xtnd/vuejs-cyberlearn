 ref(21);\r\n\r\n    let user = reactive({\r\n      name: \"Đoàn Hải Long\",\r\n      age: 22,\r\n    });\r\n    function handleClick() {\r\n      userName.value = \"Đoàn Hải Ngãi\";\r\n      userAge.value = 25;\r\n      user.name = \"Nguyễn Nhật Minh\";\r\n      user.age = 28;\r\n    }\r\n\r\n    // setTimeout(() => {\r\n    //   console.log(isRef(userName.value));\r\n    // }, 2000);\r\n    return {\r\n      userName,\r\n      userAge,\r\n      user,\r\n      handleClick,\r\n    }; //return ở đây để cho template có thể lấy được dữ liệu và sử dụng interpolation\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"]}]}